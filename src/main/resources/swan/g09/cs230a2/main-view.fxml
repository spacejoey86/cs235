<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>

<?import javafx.scene.text.Text?>
<VBox spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="swan.g09.cs230a2.ChipsChallengeController"
      stylesheets="@style.css">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
  </padding>

  <Text styleClass="title-text" text="Chip's Challenge" />
  <Button text="Play" onAction="#playGame" />
  <Button text="High Scores" onAction="#viewHighScores" />
  <Button text="Quit" onAction="#quitGame" />
</VBox>
